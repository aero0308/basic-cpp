#include <bits/stdc++.h>

using namespace std;

void main()
{
    int n, m;
    cin >> n >> m;
    int arr[n][m];
    for (int i = 0; i < n; i++)
    {
        for (int j = 0; j < n; j++)
        {
            cin >> arr[i][j];
        }
    }

    for (int j = 0; j < n; j++)
    {
        for (int i = 0; i < m; i++)
        {
            if (j < i)
            {
                cout << arr[j][i] << " ";
            }
        }
        cout << endl;
    }
}